blank_issues_enabled: true
contact_links:
  - name: Discussions
    url: https://github.com/{{ repository_namespace }}/{{ repository_name }}/discussions
    about: Ask questions and discuss with other community members
  - name: Gitter
    url: https://gitter.im/{{ repository_name }}/community
    about: Users and developers can sometimes be found on the gitter channel
  - name: Blank issue
    url: https://github.com/{{ repository_namespace }}/{{ repository_name }}/issues/new
    about: Please note that Github Discussions should be used in most cases instead
